{
  "name": "Redshift",
  "author": "Stefan Heule",
  "version": "{{ version }}.0",
  "private": true,
  "keywords": [
    "pebble-app"
  ],
  "pebble": {
    "displayName": "Redshift",
    "sdkVersion": "3",
    "targetPlatforms": [
    ## set targetPlatformsComma = joiner(",")
    ## for p in supported_platforms
       {{ targetPlatformsComma() }} "{{ p }}"
    ## endfor
    ],
    "resources": {
      "media": [
        {
          "menuIcon": true,
          "type": "png",
          "name": "IMAGE_MENU_ICON",
          "file": "images/menu_icon.png"
        },
        {
          "type": "raw",
          "name": "MAIN_FFONT",
          "file": "OpenSans-CondensedBold.ffont"
        },
        {
          "type": "raw",
          "name": "ICON_FFONT",
          "file": "FontAwesome.ffont"
        },
        {
          "type": "raw",
          "name": "WEATHER_FFONT",
          "file": "nupe2.ffont"
        }
      ]
    },
    "capabilities": [
      "configurable", "health"
    ],
    "projectType": "native",
    "uuid": "7e5267ac-798f-4953-a645-1b87a5c29d96",
    "messageKeys": {
    ## set messageKeysComma = joiner(",")
    ## for config in configuration
      {{ messageKeysComma() }} "{{ config["key"] }}": {{ config["id"] }}
    ## endfor
    ## for key in message_keys
      {{ messageKeysComma() }} "{{ key["key"] }}": {{ key["id"] }}
    ## endfor
    },
    "enableMultiJS": false,
    "watchapp": {
      "onlyShownOnCommunication": false,
      "hiddenApp": false,
      "watchface": true
    }
  },
  "devDependencies": {
    "pebble-fctx-compiler": "^1.2.1"
  },
  "dependencies": {
    "pebble-fctx": "^1.6.2"
  }
}
